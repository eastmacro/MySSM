<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="login.dao.Mapper">
    <insert id="insertUser" parameterMap="User">
        insert INTO USER (username,emailAddress,password) VALUES (#{username },#{emailAddress },#{password })
    </insert>
    <select id="getUser" resultType="User" parameterType="int">
        SELECT * FROM USER WHERE id = #{id}
    </select>
</mapper>